{
    "manifest_version": 2,
    "name": "Instagram Image Link",
    "version": "1",
    "content_scripts": [
        {
            "matches": [
                "https://www.instagram.com/*"
            ],
            "js": [
                "jquery-3.5.1.min.js",
                "content.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ]
}